{
  "name": "muwi",
  "private": true,
  "version": "0.0.0",
  "description": "Multi-Utility Writing Interface desktop app",
  "author": "ziksartin",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:web:ci": "playwright test --config playwright.config.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:electron": "MUWI_ELECTRON_BUILD=true npm run build && playwright test -c playwright.electron.config.ts",
    "perf:baseline:web": "node scripts/web/run-perf-baseline.mjs",
    "perf:budget:check": "node scripts/web/check-perf-budget.mjs",
    "bundle:budget:check": "node scripts/web/check-bundle-budget.mjs",
    "lighthouse:routes:desktop": "node scripts/web/run-lighthouse-desktop-routes.mjs",
    "lighthouse:routes:desktop:assert": "node scripts/web/run-lighthouse-desktop-routes.mjs --assert",
    "smoke:netlify": "node scripts/web/smoke-netlify.mjs",
    "audit:deps:json": "npm audit --json",
    "audit:deps:summary": "node scripts/security/summarize-npm-audit.mjs",
    "changelog": "node scripts/release/generate-changelog.mjs",
    "changelog:write": "node scripts/release/generate-changelog.mjs --output CHANGELOG.md",
    "electron:dev": "vite",
    "electron:build": "npm run build && electron-builder --config electron-builder.config.cjs",
    "electron:build:mac": "npm run build && electron-builder --config electron-builder.config.cjs --mac",
    "electron:build:win": "npm run build && electron-builder --config electron-builder.config.cjs --win",
    "electron:build:linux": "npm run build && electron-builder --config electron-builder.config.cjs --linux",
    "electron:preview": "vite build && electron .",
    "electron:preview:cleanenv": "env -u ELECTRON_RUN_AS_NODE npm run electron:preview"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@fontsource/caveat": "^5.2.8",
    "@fontsource/crimson-pro": "^5.2.8",
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/extension-underline": "^3.16.0",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "citation-js": "^0.7.22",
    "citeproc": "^2.4.63",
    "date-fns": "^4.1.0",
    "dexie": "^4.3.0",
    "dexie-react-hooks": "^4.2.0",
    "docx": "^9.5.1",
    "jspdf": "^4.2.0",
    "lucide-react": "^0.574.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "electron": "^40.0.0",
    "electron-builder": "^26.8.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "ajv": "6.14.0",
    "diff": "5.2.2",
    "markdown-it": "14.1.1",
    "@excalidraw/excalidraw": {
      "nanoid": "3.3.11"
    },
    "@excalidraw/mermaid-to-excalidraw": "git+https://github.com/vyakart/mermaid-to-excalidraw-fork-work.git#33201bbc1bc29e2d6dccaacc7d19f7e61fa53ed9"
  }
}
